        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true,
              "Time 3": true,
              "Time 4": true,
              "client_name 1": true,
              "client_name 2": true,
              "client_name 3": true,
              "client_name 4": true,
              "client_mac 1": true,
              "client_mac 2": true,
              "client_mac 3": true,
              "client_mac 4": true
            },
            "indexByName": {
              "client_name": 1,
              "client_ip": 2,
              "client_mac": 3,
              "Value #A": 4,
              "Value #B": 5,
              "Value #C": 6,
              "Value #D": 7,
              "Value #E": 8
            },
            "renameByName": {
              "Value #A": "Download Rate",
              "Value #B": "Upload Rate",
              "Value #C": "Connections",
              "Value #D": "Total Download",
              "Value #E": "Total Upload",
              "client_ip": "IP Address",
              "client_mac": "MAC Address",
              "client_name": "Client Name"
            }
          }
        }
      ],
      "type": "table"
